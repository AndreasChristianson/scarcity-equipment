    "babel-jest": "^24.9.0",
    "frisby": "^2.1.2",
    "jest": "^24.9.0",
    "nodemon": "^2.0.1",
    "onchange": "^6.1.0",
    "prettier": "^1.19.1",
    "serverless": "^1.58.0",
    "serverless-bundle": "^1.2.5",
